{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "types": [
      "vitest/importMeta",
      "vitest/globals"
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@arr/*": [
        "src/array/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}
